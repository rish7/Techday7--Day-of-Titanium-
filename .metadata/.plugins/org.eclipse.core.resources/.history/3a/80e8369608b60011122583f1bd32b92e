//Create Window
var win=Titanium.UI.createWindow({
	backgroundColor:"#5F5",
navBarHidden:true
});
//Create label
var label=Titanium.UI.createLabel({
	color:"#F00",
	top:'10px',
	left:'40%',
	text:'Welcome all'
});

//Add Label to Window
win.add(label);


//Create Textbox
var text=Titanium.UI.createTextField({
	hintText:'Enter Name',
	width:'200px',
	top:'40px',
	left:'10px',
	color:'#000'
});

win.add(text);

//Create Button
var button=Titanium.UI.createButton({
	
});

//To Open Window
win.open();

